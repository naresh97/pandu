export struct StatusBarData {
    text: string
}

export component StatusBar inherits Rectangle{
    in property <StatusBarData> data;

    height: 10%;
    background: rgb(40,40,40);
    HorizontalLayout {
        Text {
            text: root.data.text;
            vertical-alignment: center;
            color: white;
            font-size: 20px;
        }
        alignment: center;
    }
}